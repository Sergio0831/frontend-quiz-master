{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/quiz-master",
  "version": "0.1.0",
  "title": "Quiz Master Block",
  "category": "widgets",
  "description": "Display your siteâ€™s copyright date.",
  "example": {},
  "attributes": {
    "question": {
      "type": "string"
    },
    "answers": {
      "type": "array",
